{
  "editor.fontSize": 24,
  "editor.tabSize": 2,
  "editor.renderWitespace": "all",
  "editor.cursorBlinker": "expand",
  "window.zoomLevel": 0,
  "window.title": "${dirty}${separator}${rootName}${separator}${dirty}",
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.leader": "<space>",
  "vim.easymotionMarkerFontFamily": "Times new roman",
  "vim.easymotionMarkerFontSize": "16",
  "vim.easymotionMarkerHeight": 20,
  "vim.easymotionMarkerWidthPerChar": 12,
  "vim.visualstar": true,
  "vim.easymotionKeys": "asdfjkl;qweruiopzxcvnm",
  "editor.cursorSmoothCaretAnimation": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/build": true,
    "**/tmp": true,
    "**/__snapshots": true
  },
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.normalModeKeyBindings": [
    {
      "before": [
        " ",
        "w",
        "-"
      ],
      "commands": [
        ":sp"
      ]
    },
    {
      "before": [
        " ",
        "w",
        "/"
      ],
      "commands": [
        ":vsp"
      ]
    },
    {
      "before": [
        " ",
        "f",
        "s"
      ],
      "commands": [
        ":w"
      ]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "editor.fontWeight": "100",
  "editor.fontLigatures": true,
  "editor.fontFamily": "fira code",
  "workbench.colorTheme": "Hyper Term Black",
  "editor.cursorStyle": "block",
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "none",
  "terminal.integrated.shell.osx": "zsh",
  "editor.formatOnSave": true,
  "workbench.editor.enablePreview": false,
  "elixirLS.fetchDeps": false,
  "liveshare.featureSet": "insiders"
}